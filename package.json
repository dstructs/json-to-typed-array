{
  "name": "dstructs-typed-array-reviver",
  "version": "0.0.0",
  "description": "Revives a JSON-serialized typed array.",
  "author": {
    "name": "Athan Reines",
    "email": "kgryte@gmail.com"
  },
  "contributors": [
    {
      "name": "Athan Reines",
      "email": "kgryte@gmail.com"
    }
  ],
  "scripts": {
    "test": "mocha",
    "test-cov": "istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "codecov": "istanbul cover ./node_modules/.bin/_mocha --dir ./reports/codecov/coverage --report lcovonly -- -R spec && cat ./reports/codecov/coverage/lcov.info | codecov && rm -rf ./reports/codecov"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/dstructs/typed-array-reviver.git"
  },
  "keywords": [
    "compute.io",
    "dstructs",
    "typed-array",
    "typed",
    "array",
    "data",
    "structures",
    "json",
    "parse",
    "reviver",
    "revive",
    "serialization",
    "unmarshal",
    "deserialize"
  ],
  "bugs": {
    "url": "https://github.com/dstructs/typed-array-reviver/issues"
  },
  "dependencies": {
    "dstructs-array": "^1.0.0",
    "dstructs-array-dtype": "^1.0.2",
    "validate.io-array": "^1.0.6"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "codecov.io": "^0.1.5",
    "dstructs-typed-array-to-json": "git+https://github.com/dstructs/typed-array-to-json.git",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "mocha": "2.x.x",
    "utils-copy": "^1.0.0"
  },
  "license": "MIT"
}
